<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<package name="default" namespace="/" extends="struts-default">
		<action name="GetRankList"
			class="com.ruanko.music.web.GetRankList">
			<result name="success">/main.jsp</result>
			<result name="error">/error.jsp</result></action>
		<action name="Search" class="com.ruanko.music.web.Search">
			<result name="success">/search.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="SingleMusic"
			class="com.ruanko.music.web.SingleMusic">
			<result name="success">/PlayPage.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="FileUpload"
			class="com.ruanko.music.web.FileUpload">
			<result name="success">/SuccessUpload.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="AdmSearch"
			class="com.ruanko.music.web.AdmSearch">
			<result name="success">/admsearch.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="DeleteMusic"
			class="com.ruanko.music.web.DeleteMusic">
			<result name="success">/SuccessDelete.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="ShowManage"
			class="com.ruanko.music.web.ShowManage">
			<result name="success">/ManageMusic.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
		<action name="ManageMusic"
			class="com.ruanko.music.web.ManageMusic">
			<result name="success">/SuccessManage.jsp</result>
			<result name="error">/error.jsp</result>
		</action></package></struts>    
